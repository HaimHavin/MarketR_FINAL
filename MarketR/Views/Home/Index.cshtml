            $("#dvProgressBar").show();
            $("body").addClass("disable");
            debugger;
            var parm = { startDate: $('#txtSearch').val(), currencyFormat: $('#ddlCurrency :selected').val(), fileId: $('#ddlFileVersion :selected').val()};
            $.ajax({
                type: "Post",
                url: "CalculateCurrency",
                contentType: "application/json",
