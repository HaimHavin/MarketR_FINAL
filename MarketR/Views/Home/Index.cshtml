                                "</a></td> <td><a href='javascript:void(0);' onclick='GetResultView(1," + v.Col2 + ");' class='longValue1' id='longValue'>" + v.Col5 + "</a></td> <td>" + v.Col4 + "</td> <td>" + v.Col3 + "</td> <td><span id=band>" + v.Col2 + "</span></td> <td>" + v.Col1 + "</td> </tr>";
                            $('#tbodyReport1').append(report1);
                        });
                        $.each(data.report2, function (k, v) {
                            var report2 = "<tr><td>" + v.Total + "</td> <td>" + v.Other + "</td> <td>" + v.JPY + "</td> <td>" + v.CHF + "</td> <td>" + v.GBP + "</td> <td>" + v.EUR + "</td> <td>" + v.USD + "</td> <td>" + v.ILS + "</td> <td>" + v.Text3 + "</td> <td>" + v.Text2 + "</td> <td>" + v.Text1 + "</td> </tr>";
                            $('#tbodyReport2').append(report2);
                        });
                        $(".report").show(); 
                        $("body").removeClass("disable");
                    }
                    else {
                        $(".report").hide();
                        $("body").removeClass("disable");
